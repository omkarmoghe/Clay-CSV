{
  "name": "clay-csv",
  "displayName": "Clay CSV",
  "description": "CSV tools for developers.",
  "version": "0.2.2",
  "preview": true,
  "engines": {
    "vscode": "^1.84.0"
  },
  "publisher": "omkarmoghe",
  "license": "See license in LICENSE.",
  "categories": [
    "Programming Languages",
    "Language Packs"
  ],
  "keywords": [
    "csv",
    "parser",
    "tsv",
    "table",
    "comma"
  ],
  "icon": "images/icon.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/omkarmoghe/Clay-CSV.git"
  },
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "customEditors": [
      {
        "viewType": "clayCSV.tableView",
        "displayName": "CSV Table View",
        "selector": [
          {
            "filenamePattern": "*.csv"
          },
          {
            "filenamePattern": "*.tsv"
          }
        ],
        "priority": "option"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./ && cp -R src/styles out/styles",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.3",
    "@types/node": "^20.0",
    "@types/vscode": "^1.84.0",
    "@typescript-eslint/eslint-plugin": "^6.9.0",
    "@typescript-eslint/parser": "^7.3.1",
    "@vscode/test-electron": "^2.3.6",
    "eslint": "^8.52.0",
    "glob": "^10.3.10",
    "mocha": "^10.2.0",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "@fast-csv/parse": "^5.0"
  }
}
